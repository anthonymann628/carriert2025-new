<?xml version="1.0" encoding="utf-8"?>

<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <size
        android:width="64dp"
        android:height="64dp" />
    <gradient
        android:angle="180"
        android:endColor="#20CF20"
        android:startColor="#0E9E0E" />
    <stroke
        android:width="2dp"
        android:color="#80a080" />
    <padding
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp" />
    <corners
        android:bottomLeftRadius="6dp"
        android:bottomRightRadius="6dp"
        android:topLeftRadius="6dp"
        android:topRightRadius="6dp" />
</shape>